{
  "name": "parcelle-plus-server",
  "version": "1.0.0",
  "description": "Serveur backend pour l'application ParcellePlus - Version Production OVH",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test/test-server.js",
    "deploy": "chmod +x deploy-ovh.sh && ./deploy-ovh.sh",
    "backup": "/usr/local/bin/backup-parcelle.sh",
